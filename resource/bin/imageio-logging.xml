<!-- 
    Rename this file to 'logging.xml' and put it in your .maptool directory
    This will turn on additional logging during file I/O. Output goes 
    to the Java console.
-->

<!-- Turn on debugging in classes related to file IO -->
<logger name="net.rptools.maptool.client.AssetURLStreamHandler">
    <level value="DEBUG" />
    <appender-ref ref="console"/>
</logger>
