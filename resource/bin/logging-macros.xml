<!-- 
    Rename this file to 'logging.xml' and put it in your .maptool directory
    This will turn on additional logging during macro execution. Output goes 
    to the console 
-->

<!-- Turn on debugging in classes related to macro handling -->
<logger name="net.rptools.maptool.client.MapToolLineParser">
    <level value="DEBUG" />
    <appender-ref ref="console"/>
</logger>
<logger name="net.rptools.maptool.client.macro.MacroManager">
    <level value="DEBUG" />
    <appender-ref ref="console"/>
</logger>
<logger name="net.rptools.maptool.client.model.MacroButtonProperties">
    <level value="DEBUG" />
    <appender-ref ref="console"/>
</logger>
<logger name="net.rptools.maptool.client.model.Token">
    <level value="DEBUG" />
    <appender-ref ref="console"/>
</logger>




